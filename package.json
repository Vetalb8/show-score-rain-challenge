{
  "name": "show-score-game",
  "version": "0.1.0",
  "description": "test task",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build:prod": "npm run clean && webpack --env production -p --progress",
    "start": "webpack-dev-server --env development",
    "test:coverage": "node testRunner.js --coverage",
    "test": "node testRunner.js",
    "lint:js": "eslint ./src --ext .js --ext .jsx --ignore-path .gitignore --ignore-pattern dist --cache",
    "lint:style": "stylelint ./src/**/*.scss"
  },
  "keywords": [
    "react",
    "reactjs",
    "hot",
    "reload",
    "hmr",
    "redux",
    "es6",
    "babel",
    "webpack"
  ],
  "eslintConfig": {
    "root": true,
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "es6": true,
      "commonjs": true,
      "node": true
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "no-debugger": 0,
      "no-console": 0,
      "no-unused-vars": 0,
      "react/display-name": 0,
      "react/prop-types": 0,
      "react/no-multi-comp": 0,
      "quotes": [
        "error",
        "single"
      ],
      "semi": 0,
      "indent": [
        "error",
        2
      ],
      "react/no-render-return-value": 0,
      "no-case-declarations": 0
    }
  },
  "author": "Vkudla",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.1.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "classnames": "^2.2.5",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "npm-install-webpack-plugin": "^4.0.5",
    "postcss-loader": "^2.0.6",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-act": "^1.4.1",
    "redux-logger": "^3.0.6",
    "resolve-url": "^0.2.1",
    "resolve-url-loader": "^2.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "stats-webpack-plugin": "^0.6.1",
    "style-loader": "^0.13.2",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.3",
    "url-loader": "^0.6.2",
    "webpack": "^2.2.1",
    "webpack-chunk-hash": "^0.5.0",
    "webpack-merge": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.2.1",
    "stylelint": "^8.2.0",
    "stylelint-scss": "^2.1.0",
    "webpack-config-utils": "^2.3.0",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-hot-middleware": "^2.20.0",
    "webpack-validator": "^3.0.0"
  }
}
